{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "ignore": ["examples/"],
  "env": {
    "browser": false,
    "node": true,
    "mocha": true
  },
  "rules": {
    "camelcase": 0,
    "class-methods-use-this": 0,
    "generator-star-spacing": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "indent": [2, 4, {"SwitchCase": 1}],
    "linebreak-style": 0,
    "max-len": [2, 250],
    "no-cond-assign": 0,
    "no-constant-condition": 0,
    "no-continue": 0,
    "no-else-return": 0,
    "no-empty": 0,
    "no-lonely-if": 0,
    "no-loop-func": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": [0, "ForOfStatement"],
    "no-return-assign": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-useless-constructor": 0,
    "no-useless-escape": 0,
    "new-cap": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 0
  }
}