{
  "name": "renlang",
  "version": "0.0.1",
  "description": "The Ren Programming Language",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn run clean && tsc",
    "circular-deps": "yarn run build && madge --circular dist/src",
    "deps-graph": "yarn run build && madge --image graph.svg dist/src",
    "test-ts": "yarn run build && mocha dist/test/**/*.js",
    "coverage": "yarn run build && istanbul cover node_modules/mocha/bin/_mocha dist/test/**/*.js",
    "test": "babel-node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha test/**/*.js",
    "test-debug": "node node_modules/babel-cli/bin/babel-node.js node_modules/mocha/bin/_mocha debug test/**/*.js",
    "test-inspect": "node node_modules/babel-cli/bin/babel-node.js node_modules/mocha/bin/_mocha --inspect --debug-brk test/**/*.js"
  },
  "repository": "https://github.com/jchitel/renlang.git",
  "author": "Jake Chitel <jchitel@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-subset": "^1.3.1",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.34",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.2",
    "chai-subset": "^1.6.0",
    "eslint": "3",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "madge": "^2.2.0",
    "mocha": "^3.4.2",
    "tslib": "^1.8.0",
    "tslint": "^5.7.0",
    "typescript": "^2.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  }
}
